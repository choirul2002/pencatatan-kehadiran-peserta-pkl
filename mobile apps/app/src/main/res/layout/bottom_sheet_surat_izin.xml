<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bottom_sheet_top_corve">

    <View
        android:id="@+id/divider10"
        android:layout_width="50dp"
        android:layout_height="5dp"
        android:layout_marginTop="24dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView52"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:text="Surat Izin"
        android:textColor="#000000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider10" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/camear"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="24dp"
        android:src="@color/white"
        app:civ_border_color="@color/light_grey"
        app:civ_border_width="1dp"
        app:layout_constraintStart_toStartOf="@+id/textView52"
        app:layout_constraintTop_toBottomOf="@+id/textView52" />


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/vector"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="24dp"
        android:src="@color/white"
        app:civ_border_color="@color/light_grey"
        app:civ_border_width="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/camear"
        app:layout_constraintStart_toEndOf="@+id/camear"
        app:layout_constraintTop_toTopOf="@+id/camear" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="24dp"
        android:text="Camera"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/camear"
        app:layout_constraintStart_toStartOf="@+id/camear"
        app:layout_constraintTop_toBottomOf="@+id/camear" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="24dp"
        android:text="Storage"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/vector"
        app:layout_constraintStart_toStartOf="@+id/vector"
        app:layout_constraintTop_toBottomOf="@+id/camear" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/ic_baseline_camera_alt_24"
        app:layout_constraintBottom_toBottomOf="@+id/camear"
        app:layout_constraintEnd_toEndOf="@+id/camear"
        app:layout_constraintStart_toStartOf="@+id/camear"
        app:layout_constraintTop_toTopOf="@+id/camear" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/ic_baseline_sd_storage_24"
        app:layout_constraintBottom_toBottomOf="@+id/vector"
        app:layout_constraintEnd_toEndOf="@+id/vector"
        app:layout_constraintStart_toStartOf="@+id/vector"
        app:layout_constraintTop_toTopOf="@+id/vector" />

</androidx.constraintlayout.widget.ConstraintLayout>